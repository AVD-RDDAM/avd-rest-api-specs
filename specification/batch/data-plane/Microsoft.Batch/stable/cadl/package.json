{
    "name": "batch-cadl-root",
    "author": "Microsoft Corporation",
    "description": "Root Batch Cadl project for copying the Cadl spec files into the language SDK sub directory",
    "license": "MIT",
    "devDependencies": {
        "npm-run-all": "^4.1.5"
      },
    "scripts": {
        "copycadl": "run-p copycadl:*",
        "copycadl:typescript": "cp *cadl typescript-proj",
        "copycadl:python": "cp *cadl python-proj",
        "copycadl:java": "cp *cadl java-proj",
        "copycadl:csharp": "cp *cadl csharp-proj"
    },
    "private": true
  }
