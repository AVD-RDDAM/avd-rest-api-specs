{
  "name": "@cadl-api-spec/azure-language-textanalytics",
  "version": "0.1.0",
  "author": "Microsoft Corporation",
  "description": "Text Analytics",
  "license": "MIT",
  "main": "dist/src/index.js",
  "cadlMain": "main.cadl",
  "exports": {
    ".": "./dist/src/index.js",
    "./testing": "./dist/src/testing/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/src/index.d.ts"
      ],
      "testing": [
        "./dist/src/testing/index.d.ts"
      ]
    }
  },
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "clean": "rimraf ./dist ./temp",
    "build": "tsc -p . && npm run lint-cadl-library",
    "watch": "tsc -p . --watch",
    "lint-cadl-library": "cadl compile . --import @cadl-lang/library-linter",
    "test": "mocha",
    "test-official": "c8 mocha --forbid-only"
  },
  "files": [
    "*.cadl",
    "dist/**",
    "!dist/test/**"
  ],
  "peerDependencies": {
    "@cadl-lang/compiler": "~0.35.0",
    "@cadl-lang/rest": "~0.17.0",
    "@azure-tools/cadl-autorest": "~0.20.0",
    "@azure-tools/cadl-azure-core": "~0.7.0"
  },
  "devDependencies": {
    "@cadl-lang/compiler": "~0.35.0",
    "@cadl-lang/library-linter": "~0.2.0",
    "@cadl-lang/rest": "~0.17.0",
    "@cadl-lang/versioning": "~0.8.0",
    "@cadl-lang/eslint-plugin": "~0.1.1",
    "@azure-tools/cadl-autorest": "~0.20.0",
    "@azure-tools/cadl-azure-core": "~0.7.0",
    "@types/mocha": "~9.1.0",
    "@types/node": "~16.0.3",
    "mocha": "~9.2.0",
    "c8": "~7.11.0",
    "rimraf": "~3.0.2",
    "typescript": "~4.8.2"
  }
}
