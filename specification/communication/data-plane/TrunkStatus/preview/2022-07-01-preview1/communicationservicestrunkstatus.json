  "definitions": {
    "ListOfTrunksHealth": {
      "description": "Health of SIP trunks.",
      "type": "object",
      "properties": {
        "value": {
          "description": "Represents an array of SIP trunks health.",
          "maxLength": 250,
          "type": "array",
          "items": {
            "$ref": "#/definitions/TrunkHealth"
          }
        }
      },
      "required": [
        "value"
      ]
    },
    "TrunkHealth": {
      "description": "Represents health of a SIP trunk for routing calls.",
      "type": "object",
      "required": [
        "fqdn",
        "tls",
        "ping",
        "overallStatus"
      ],
      "properties": {
        "fqdn": {
          "description": "Trunk's FQDN of the paired SBC",
          "maxLength": 256,
          "type": "string"
        },
        "tls": {
          "description": "The status of the TLS connections between Direct Routing and the SBC",
          "maxLength": 256,
          "type": "string",
          "enum": [
            "NoData",
            "OK",
            "CertExpiring",
            "CertExpired"
          ]
        },
        "ping": {
          "description": "The status of options message send by SBC",
          "maxLength": 256,
          "type": "string",
          "enum": [
            "NoData",
            "OK",
            "PingError",
            "PingExpired"
          ]
        },
        "overallStatus": {
          "description": "The overall status of SBC",
          "maxLength": 256,
          "type": "string",
          "enum": [
            "NoData",
            "Active",
            "InactiveNoRecentPings",
            "InactiveNoRecentPingsAndCalls",
            "InactiveNoRecentCalls"
          ]
        }
      }
    }
  },
