          }
        }
      },
      "required": [
        "value"
      ]
    },
    "TrunkHealth": {
      "description": "Represents health state of a SIP trunk for routing calls.",
      "type": "object",
      "required": [
        "fqdn",
        "tls",
        "ping",
        "trunkOverallStatus"
      ],
      "properties": {
        "fqdn": {
          "description": "Trunk's FQDN of the paired SBC",
          "maxLength": 256,
          "type": "string"
        },
        "tls": {
          "type": "object",
          "description": "The status of the TLS connections between Direct Routing and the SBC",
          "required": [
            "tlsStatus"
          ],
          "properties": {
            "tlsStatus": {
              "type": "string",
              "description": "The status of the TLS connections between Direct Routing and the SBC",
              "enum": [
                "Unknown",
                "Ok",
                "Warning",
                "Error"
              ],
              "x-ms-enum": {
                "name": "Tls",
                "modelAsString": true,
                "values": [
                  {
                    "value": "Unknown",
                    "description": "Indicates an unknown health status."
                  },
                  {
                    "value": "Ok",
                    "description": "Indicates the health state is okay."
                  },
                  {
                    "value": "Warning",
                    "description": "Indicates the health state is at a warning level."
                  },
                  {
                    "value": "Error",
                    "description": "Indicates the health state is at an error level."
                  }
                ]
              }
            },
            "certificateStatus": {
              "type": "string",
              "description": "The status of the SBC certificate",
              "enum": [
                "Ok",
                "Expiring",
                "Expired"
              ],
              "x-ms-enum": {
                "name": "certificateStatus",
                "modelAsString": true,
                "values": [
                  {
                    "value": "Ok",
                    "description": "Indicates the certificate is okay."
                  },
                  {
                    "value": "Expiring",
                    "description": "Indicates the certificate is expiring."
                  },
                  {
                    "value": "Expired",
                    "description": "Indicates the certificate is expired."
                  }
                ]
              }
            }
          }
        },
        "ping": {
          "type": "string",
          "description": "The status of options message sent by SBC",
          "enum": [
            "Unknown",
            "Ok",
            "Warning",
            "Error"
          ],
          "x-ms-enum": {
            "name": "ping",
            "modelAsString": true,
            "values": [
              {
                "value": "Unknown",
                "description": "Indicates an unknown health status."
              },
              {
                "value": "Ok",
                "description": "Indicates the health state is okay."
              },
              {
                "value": "Warning",
                "description": "Indicates the health state is at a warning level."
              },
              {
                "value": "Error",
                "description": "Indicates the health state is at an error level."
              }
            ]
          }
        },
        "trunkOverallStatus": {
          "type": "object",
          "description": "The overall status of SBC",
          "required": [
            "overallStatus"
          ],
          "properties": {
            "overallStatus": {
              "type": "string",
              "description": "The overall status of SBC",
              "enum": [
                "Unknown",
                "Active",
                "Inactive"
              ],
              "x-ms-enum": {
                "name": "overallStatus",
                "modelAsString": true,
                "values": [
                  {
                    "value": "Unknown",
                    "description": "Indicates an unknown status."
                  },
                  {
                    "value": "Active",
                    "description": "Indicates the SBC is active."
                  },
                  {
                    "value": "Inactive",
                    "description": "Indicates the SBC is inactive."
                  }
                ]
              }
            },
            "inactiveReasons ": {
              "type": "string",
              "description": "The inactive status reasons",
              "enum": [
                "NoRecentCalls",
                "NoRecentPings"
              ],
              "x-ms-enum": {
                "name": "inactiveReasons ",
                "modelAsString": true,
                "values": [
                  {
                    "value": "NoRecentCalls",
                    "description": "No recent pings."
                  },
                  {
                    "value": "NoRecentCalls",
                    "description": "No recent calls."
                  }
                ]
              }
            }
          }
        }
      }
    }
